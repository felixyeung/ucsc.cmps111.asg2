all : prog_A.c prog_B.c prog_C.c prog_D.c noStarvingWriters.c
	gcc -o A prog_A.c
	gcc -o B prog_B.c
	gcc -o C prog_C.c
	gcc -o D prog_D.c
	gcc -o noStarvingWriters noStarvingWriters.c

clean :
	rm -f A B C D noStarvingWriters.c
